<odoo>
    <record id="equ_checkline_view_form" model="ir.ui.view">
        <field name="name">Equ check Form</field>
        <field name="model">equcheck.line</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <group>
                            <field name="state_id" />
                            <field name="owner_id" />
                            <field name="resperson_id" />
                            <field name="sysperson_id" />
                        </group>
                        <group>
                            <field name="sort_id" />
                            <field name="department_id" />
                            <field name="money" />
                            <field name="receiving_data" />
                        </group>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

